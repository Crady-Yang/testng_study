[
  {
    "description": "模拟没有参数的get请求",
    "request":{
      "uri":"/getdemo",
      "method":"get"
    },
    "response":{
      "text":"这是一个没有参数的get请求"
    }
  },
  {
    "description": "模拟到参数的get请求",
    "request":{
      "uri":"/getwithparam",
      "method":"get",
      "queries": {
        "name":"老王",
        "age":"10"
      }
    },
    "response":{
      "text":"我胡汉三又回来啦！！！！"
    }
  }
]